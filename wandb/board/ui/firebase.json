{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "build",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [{
      "source": "/service-worker.js",
      "headers": [{
        "key": "Cache-Control",
        "value": "private, max-age=0, no-cache"
      }]
      },{
      "source" : "/index.html",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "private, max-age=0, no-cache"
      } ]
    }]
  }
}
